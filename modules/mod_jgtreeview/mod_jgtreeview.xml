   <language tag="en-GB">languages/en-GB/en-GB.mod_jgtreeview.ini</language>
   <language tag="de-DE">languages/de-DE/de-DE.mod_jgtreeview.ini</language>
   <language tag="nl-NL">languages/nl-NL/nl-NL.mod_jgtreeview.ini</language>
  </languages>
  <params>
    <param type="spacer" default="JG_TREEVIEW_COMMON_PARAMETER_LBL" label="" description="" />  
    <param name="moduleclass_sfx" type="text" default="" label="JG_TREEVIEW_MODULE_CLASS_SUFFIX_LBL" description="JG_TREEVIEW_MODULE_CLASS_SUFFIX_DESC" />
    <param name="cfg_itemid" type="text" default="" label="JG_TREEVIEW_ITEMID_LBL" description="JG_TREEVIEW_ITEMID_DESC"/>  
    <param name="cfg_root_catid" type="text" default="0" label="JG_TREEVIEW_ROOT_CATID_LBL" description="JG_TREEVIEW_ROOT_CATID_DESC" />
    <param name="cfg_name" type="text" default="" label="JG_TREEVIEW_NAME_LBL" description="JG_TREEVIEW_NAME_DESC" />
    <param name="cfg_sort_cat" type="list" default="0" label="JG_TREEVIEW_SORT_CAT_LBL" description="JG_TREEVIEW_SORT_CAT_DESC">
      <option value="name ASC">JG_TREEVIEW_SORT_NAME_ASC_LBL</option>
      <option value="name DESC">JG_TREEVIEW_SORT_NAME_DESC_LBL</option>
      <option value="cid ASC">JG_TREEVIEW_SORT_DATE_ASC_LBL</option>
      <option value="cid DESC">JG_TREEVIEW_SORT_DATE_DESC_LBL</option>
      <option value="ordering ASC">JG_TREEVIEW_SORT_ORDERING_ASC_LBL</option>
      <option value="ordering DESC">JG_TREEVIEW_SORT_ORDERING_DESC_LBL</option>
    </param>
    <param name="cfg_sort_subcat" type="list" default="0" label="JG_TREEVIEW_SORT_SUBCAT_LBL" description="JG_TREEVIEW_SORT_SUBCAT_DESC">
      <option value="name ASC">JG_TREEVIEW_SORT_NAME_ASC_LBL</option>
      <option value="name DESC">JG_TREEVIEW_SORT_NAME_DESC_LBL</option>
      <option value="cid ASC">JG_TREEVIEW_SORT_DATE_ASC_LBL</option>
      <option value="cid DESC">JG_TREEVIEW_SORT_DATE_DESC_LBL</option>
      <option value="ordering ASC">JG_TREEVIEW_SORT_ORDERING_ASC_LBL</option>
      <option value="ordering DESC">JG_TREEVIEW_SORT_ORDERING_DESC_LBL</option>
    </param>
    <param name="cfg_blacklist_cats" type="text" default="" label="JG_TREEVIEW_BLACKLIST_CATS_LBL" description="JG_TREEVIEW_BLACKLIST_CATS_DESC" />
    <param name="cfg_showhidden" type="radio" default="0" label="JG_TREEVIEW_SHOWHIDDEN_LBL" description="JG_TREEVIEW_SHOWHIDDEN_DESC">
      <option value="0">JG_TREEVIEW_NO_LBL</option>
      <option value="1">JG_TREEVIEW_YES_LBL</option>
    </param>            
    <param name="cfg_max_len_catname" type="text" default="0" label="JG_TREEVIEW_MAX_LEN_CATNAME_LBL" description="JG_TREEVIEW_MAX_LEN_CATNAME_DESC" />
    <param name="cfg_show_open_close_all" type="radio" default="1" label="JG_TREEVIEW_SHOW_OPEN_CLOSE_ALL_LBL" description="JG_TREEVIEW_SHOW_OPEN_CLOSE_ALL_DESC">
      <option value="0">JG_TREEVIEW_NO_LBL</option>
      <option value="1">JG_TREEVIEW_YES_LBL</option>
    </param>
    <param name="cfg_show_always_expanded" type="radio" default="0" label="JG_TREEVIEW_SHOW_ALWAYS_EXPANDED_LBL" description="JG_TREEVIEW_SHOW_ALWAYS_EXPANDED_DESC">
      <option value="0">JG_TREEVIEW_NO_LBL</option>
      <option value="1">JG_TREEVIEW_YES_LBL</option>
    </param>    
    <param name="cfg_show_credits" type="radio" default="1" label="JG_TREEVIEW_SHOW_CREDITS_LBL" description="JG_TREEVIEW_SHOW_CREDITS_DESC">
      <option value="0">JG_TREEVIEW_NO_LBL</option>
      <option value="1">JG_TREEVIEW_YES_LBL</option>
    </param>
    <param type="spacer" default="JG_TREEVIEW_VIEW_PARAMETER_LBL" label="" description="" />
    <param name="cfg_icon_theme" type="list" default="" label="JG_TREEVIEW_ICON_THEME_LBL" description="JG_TREEVIEW_ICON_THEME_DESC">
      <option value="">JG_TREEVIEW_ICON_THEME_0_LBL</option>
      <option value="blue_arrows">JG_TREEVIEW_ICON_THEME_1_LBL</option>
      <option value="orange_arrows">JG_TREEVIEW_ICON_THEME_2_LBL</option>
      <option value="red_flowers">JG_TREEVIEW_ICON_THEME_3_LBL</option>
    </param>        
    <param name="cfg_grid" type="radio" default="1" label="JG_TREEVIEW_GRID_LBL" description="JG_TREEVIEW_GRID_DESC">
      <option value="0">JG_TREEVIEW_NO_LBL</option>
      <option value="1">JG_TREEVIEW_YES_LBL</option>
    </param>
    <param name="cfg_bgcolor_active_node" type="text" default="#C0D2EC" label="JG_TREEVIEW_BGCOLOR_ACTIVE_NODE_LBL" description="JG_TREEVIEW_BGCOLOR_ACTIVE_NODE_DESC" />
    <param name="cfg_disable_new" type="radio" default="0" label="JG_TREEVIEW_DISABLE_NEW_LBL" description="JG_TREEVIEW_DISABLE_NEW_DESC">
      <option value="0">JG_TREEVIEW_NO_LBL</option>
      <option value="1">JG_TREEVIEW_YES_LBL</option>
    </param>
  </params>
</install>